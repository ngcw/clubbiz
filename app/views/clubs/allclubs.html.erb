<h1>All Clubs</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clubs.each do |club| %>
	  <%if club.approved%>
	    <tr>
	      <td><%= link_to club.name, club %></td>
	      <td><%= club.description %></td>
	    </tr>
      <%end%>
    <% end %>
  </tbody>
</table>



<%= link_to 'Start a Club', new_club_path %>
