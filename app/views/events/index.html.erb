<h1>Listing events</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Memberonly</th>
      <th>Date</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.memberOnly %></td>
        <td><%= event.date %></td>
        <td><%= event.place %></td>
  
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>



