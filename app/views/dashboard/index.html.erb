<div class="container">
  <h1>Welcome to your Dashboard!</h1>
  <div class="row">
    <div class="col-md-6 col-xs-12">
      <h3> Your Clubs </h3>
      <div class="list-group">
        <% @clubs.each do |club| %>
          <%= link_to club.name, club, :class => "list-group-item"%>
        <% end %>
      </div>
    </div>
    <div class="col-md-6 col-xs-12">
      <h3> Your Events </h3>
      <div class="list-group">
        <% @events.each do |event| %>
          <%= link_to event.name, event, :class => "list-group-item" %>
        <% end %>
      </div>
    </div>
  </div>
</div>


