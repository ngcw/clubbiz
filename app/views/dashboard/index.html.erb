<div class="container">
  <h1>Welcome to your Dashboard!</h1>
  <div class="row">
    <div class="col-md-6 col-xs-12">
      <h3> Your Clubs </h3>
      <div class="list-group">
        <% @clubs.each do |club| %>
          <%= link_to club.name, club, :class => "list-group-item" %>
          <p class="list-group-item-text"> <%= club.description %> </p>
        <% end %>
      </div>
    </div>
  </div>
</div>


